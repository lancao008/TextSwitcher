<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_mac"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mvp.libin.textswitch.MACActivity">

    <TextView
        android:text="MAC"
        android:textColor="#000000"
        android:id="@+id/tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_mac"
        app:layout_constraintTop_toTopOf="@+id/activity_mac"
        app:layout_constraintLeft_toLeftOf="@+id/activity_mac"
        app:layout_constraintRight_toRightOf="@+id/activity_mac"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:textSize="20sp"
        android:textColor="#000000"
        android:text="SSID"
        android:id="@+id/SSID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/tv"
        app:layout_constraintLeft_toLeftOf="@+id/activity_mac"
        app:layout_constraintBottom_toBottomOf="@+id/activity_mac"
        android:layout_marginBottom="16dp"
        app:layout_constraintRight_toRightOf="@+id/activity_mac"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

</android.support.constraint.ConstraintLayout>
